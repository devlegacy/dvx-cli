{
  "name": "@devexteam/dvx-cli",
  "version": "2.0.0",
  "description": "CLI that encapsulates utilities for image processing and assets in frontend development.",
  "license": "MIT",
  "main": "bin/index.js",
  "bin": {
    "dvx": "bin/index.js"
  },
  "scripts": {
    "jest:init": "jest --init",
    "jest:watch": "jest --watchAll",
    "test": "jest",
    "tsc:watch": "rm -Rf ./build && tsc --watch --pretty",
    "tsc:build": "rm -Rf ./build && tsc --build --pretty",
    "build": "rm -Rf ./build && npx ncc build ./src/index.ts -o ./bin/ -m",
    "webpack:build": "webpack --config webpack.config.js --mode=production",
    "package:update": "ncu -u && npm i --legacy-peer-deps && npm audit fix --legacy-peer-deps"
  },
  "keywords": [
    "Devexteam",
    "cli",
    "minify",
    "tools",
    "frontend"
  ],
  "files": [
    "README.MD"
  ],
  "directories": {
    "bin": "bin"
  },
  "author": "Samuel R. <samuel@devexteam.com>",
  "private": true,
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/htmlhint": "^0.9.2",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-svgo": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/node-notifier": "^8.0.0",
    "@types/sharp": "^0.28.1",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^16.0.1",
    "@vercel/ncc": "^0.28.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "beeper": "^3.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "htmlhint": "^0.14.2",
    "htmlhint-stylish": "^1.0.3",
    "imagemin": "^8.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "imagemin-zopfli": "^7.0.0",
    "node-notifier": "^9.0.1",
    "sharp": "^0.28.2",
    "shelljs": "^0.8.4",
    "yargs": "^17.0.1"
  },
  "engines": {
    "node": ">=12.18.3 <=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlegacy/dvx-cli.git"
  },
  "bugs": {
    "url": "https://github.com/devlegacy/dvx-cli/issues"
  },
  "homepage": "https://github.com/devlegacy/dvx-cli#readme"
}
